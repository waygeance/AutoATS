{
  "name": "autoats",
  "version": "1.0.0",
  "description": "AI-powered resume builder that optimizes your resume for specific job descriptions",
  "private": true,
  "workspaces": ["apps/web", "packages/api", "packages/latex"],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "start": "npm run start --workspace=apps/web",
    "test": "npm run test --workspace=apps/web",
    "lint": "npm run lint --workspace=apps/web",
    "setup": "./infra/setup.sh",
    "clean": "npm run clean --workspaces && rm -rf .next node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/autoats.git"
  },
  "keywords": ["resume", "ats", "ai", "job-description", "nextjs", "ollama", "latex"],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
